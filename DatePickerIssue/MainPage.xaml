<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:datePickerIssue="clr-namespace:DatePickerIssue"
             x:Class="DatePickerIssue.MainPage"
             x:DataType="datePickerIssue:MainPageViewModel">
            <DatePicker Date="{Binding Date}" MaximumDate="{Binding DateMax}" MinimumDate="{Binding DateMin}"></DatePicker>

</ContentPage>
